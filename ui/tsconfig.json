{
  "compileOnSave": false,
  "buildOnSave": false,
  "compilerOptions": {
    "downlevelIteration": true,
    "experimentalDecorators": true,
	"allowJs": true,
	"baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": false,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"]
  }
}
